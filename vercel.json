{
  "version": 2,
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/public/styles.css",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/script.js", 
      "dest": "/public/script.js",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/proxy",
      "dest": "/server.js"
    },
    {
      "src": "/index.html",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ]
}
